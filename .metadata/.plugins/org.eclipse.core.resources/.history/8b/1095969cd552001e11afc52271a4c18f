package com.aurionpro.model;

public class LaptopBuilder {

	private Laptop laptop;
	
	public LaptopBuilder(Laptop laptop) {
		super();
		this.laptop = laptop;
	}
	
	private LaptopBuilder addBrand(String brand) {
		this.laptop.setBrand(brand);
		return this;
	}
	
	private LaptopBuilder addHdd(String hdd) {
		this.laptop.setHdd(hdd);
		return this;
	}
	
}
